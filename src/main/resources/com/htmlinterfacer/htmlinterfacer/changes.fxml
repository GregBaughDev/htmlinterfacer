<?import javafx.scene.control.Button?>
<GridPane fx:controller="com.htmlinterfacer.htmlinterfacer.controller.ChangesController" xmlns:fx="http://javafx.com/fxml"
          alignment="CENTER" hgap="10" vgap="10" prefHeight="800" prefWidth="1000">

    <VBox GridPane.columnIndex="6" GridPane.columnSpan="4" >
